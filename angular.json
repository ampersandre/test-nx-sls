{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "sample",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "css"
    }
  },
  "projects": {
    "sample": {
      "projectType": "application",
      "root": "apps/sample",
      "sourceRoot": "apps/sample/src",
      "prefix": "workspace",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/sample",
            "index": "apps/sample/src/index.html",
            "main": "apps/sample/src/main.ts",
            "polyfills": "apps/sample/src/polyfills.ts",
            "tsConfig": "apps/sample/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/sample/src/favicon.ico",
              "apps/sample/src/assets"
            ],
            "styles": [
              "apps/sample/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/sample/src/environments/environment.ts",
                  "with": "apps/sample/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "sample:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "sample:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "sample:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/sample/src/**/*.ts",
              "apps/sample/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/sample"
          ],
          "options": {
            "jestConfig": "apps/sample/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "sample-e2e": {
      "root": "apps/sample-e2e",
      "sourceRoot": "apps/sample-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/sample-e2e/cypress.json",
            "tsConfig": "apps/sample-e2e/tsconfig.e2e.json",
            "devServerTarget": "sample:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "sample:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/sample-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "nest-api": {
      "root": "apps/nest-api",
      "sourceRoot": "apps/nest-api/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@flowaccount/nx-serverless:build",
          "options": {
            "outputPath": "dist/apps/nest-api",
            "package": "apps/nest-api",
            "serverlessConfig": "apps/nest-api/serverless.yml",
            "servicePath": "apps/nest-api",
            "tsConfig": "apps/nest-api/tsconfig.app.json",
            "provider": "aws",
            "processEnvironmentFile": "env.json"
          },
          "configurations": {
            "dev": {
              "optimization": false,
              "sourceMap": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "production": {
              "optimization": true,
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/nest-api/environment.ts",
                  "with": "apps/nest-api/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@flowaccount/nx-serverless:offline",
          "options": {
            "buildTarget": "nest-api:build",
            "config": "apps/nest-api/serverless.yml",
            "location": "dist/apps/nest-api"
          },
          "configurations": {
            "dev": {
              "buildTarget": "nest-api:build:dev"
            },
            "production": {
              "buildTarget": "nest-api:build:production"
            }
          }
        },
        "deploy": {
          "builder": "@flowaccount/nx-serverless:deploy",
          "options": {
            "buildTarget": "nest-api:build:production",
            "config": "apps/nest-api/serverless.yml",
            "location": "dist/apps/nest-api",
            "package": "dist/apps/nest-api"
          }
        },
        "destroy": {
          "builder": "@flowaccount/nx-serverless:destroy",
          "options": {
            "buildTarget": "nest-api:build:production",
            "config": "apps/nest-api/serverless.yml",
            "location": "dist/apps/nest-api",
            "package": "dist/apps/nest-api"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/nest-api/tsconfig.app.json",
              "apps/nest-api/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/nest-api/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/nest-api"],
          "options": {
            "jestConfig": "apps/nest-api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
